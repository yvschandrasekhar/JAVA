package com.Bubble.sort;

public class BubbleSort_2 {

	public static void main(String[] args) {
		int []a= {15,8,2,16,7,3};
		for(int i=0;i<a.length-3;i++)
		{
			for(int j=0;j<a.length-1;j++)
			{
				int temp=a[j];
				if(a[j]>a[j+1])
				{
					temp=a[j];
					a[j]=a[j+1];
					a[j+1]=temp;
				}
				System.out.println(temp);
			}
		}
	}

}
